       	OWLEntity entityc = df.getOWLEntity(EntityType.CLASS, IRI.create("#C"));
       	arguments.add((OWLClassExpression) entityr);
       	arguments.add((OWLClassExpression) entityc);
       

       	OWLAxiom axiom = df.getOWLEquivalentClassesAxiom(arguments);
       	manager.addAxiom(ontology, axiom);
       	
       	OWLClass atomicConcept=df.getOWLClass(IRI.create("#AtomicConcept"));
       	OWLObjectProperty hasModelingFeatures=df.getOWLObjectProperty
       			(IRI.create("#hasModelingFeatures"));
       	OWLClassExpression hasOnlyModelingFAll=df.getOWLObjectAllValuesFrom(hasModelingFeatures, atomicConcept);
       	
       	OWLClass alc=df.getOWLClass(IRI.create("#ALCCC"));
